itor,\n:host,\n.editor .gutter,\n:host .gutter` => `.editor,atom-text-editor,\n.editor .gutter,atom-text-editor .gutter`\n\n* `.editor.is-focused .cursor,\n:host.is-focused .cursor` => `.editor.is-focused .cursor,atom-text-editor.is-focused .cursor`\n\n* `.editor.is-focused .selection .region,\n:host.is-focused .selection .region` => `.editor.is-focused .selection .region,atom-text-editor.is-focused .selection .region`\n\n* `.editor.is-focused .line-number.cursor-line-no-selection,\n:host.is-focused .line-number.cursor-line-no-selection,\n.editor.is-focused .line.cursor-line,\n:host.is-focused .line.cursor-line` => `.editor.is-focused .line-number.cursor-line-no-selection,atom-text-editor.is-focused .line-number.cursor-line-no-selection,\n.editor.is-focused .line.cursor-line,atom-text-editor.is-focused .line.cursor-line`\n\n* `.editor .comment,\n:host .comment` => `.editor .syntax--comment,atom-text-editor .syntax--comment`\n\n* `.editor .string,\n:host .string` => `.editor .syntax--string,atom-text-editor .syntax--string`\n\n* `.editor .constant.numeric,\n:host .constant.numeric` => `.editor .syntax--constant.syntax--numeric,atom-text-editor .syntax--constant.syntax--numeric`\n\n* `.editor .constant.language,\n:host .constant.language` => `.editor .syntax--constant.syntax--language,atom-text-editor .syntax--constant.syntax--language`\n\n* `.editor .constant.character,\n:host .constant.character,\n.editor .constant.other,\n:host .constant.other` => `.editor .syntax--constant.syntax--character,atom-text-editor .syntax--constant.syntax--character,\n.editor .syntax--constant.syntax--other,atom-text-editor .syntax--constant.syntax--other`\n\n* `.editor .keyword,\n:host .keyword` => `.editor .syntax--keyword,atom-text-editor .syntax--keyword`\n\n* `.editor .storage,\n:host .storage` => `.editor .syntax--storage,atom-text-editor .syntax--storage`\n\n* `.editor .storage.type,\n:host .storage.type` => `.editor .syntax--storage.syntax--type,atom-text-editor .syntax--storage.